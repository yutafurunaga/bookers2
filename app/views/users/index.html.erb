<%= render 'books/list', user: @user, book: @book %>

<div class="col-md-8 offset-md-1">
      <h2>Users</h2>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
        <% @users.each do |user| %>
        <tr>
           <td><%= image_tag @user.get_image, size:'100x100' %></td>
           <td><%= user.name %></td>
           <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
        <% end %>
      </thead>
    </table>
 </div>